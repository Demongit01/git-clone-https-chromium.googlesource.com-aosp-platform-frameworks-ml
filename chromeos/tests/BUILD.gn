import("//common-mk/pkg_config.gni")

nn_src = "//aosp/frameworks/ml"
vts_src = "//aosp/hardware/interfaces/neuralnetworks"

group("all") {
  deps = [
    ":vts",
    ":cts",
  ]
}

group("vts") {
  deps = [
    ":cros_nnapi_vts_1_0",
    ":cros_nnapi_vts_1_1",
    ":cros_nnapi_vts_1_2",
    ":cros_nnapi_vts_1_3",
  ]
}

group("cts") {
  deps = [
    ":cros_nnapi_cts",
  ]
}

pkg_config("vts_defaults") {
  include_dirs = [
    "${vts_src}/1.0/vts/functional/include",
    "${nn_src}/common/include",
    "${nn_src}/common/include/nnapi",
    "${nn_src}/common/types/include/nnapi",
    "${nn_src}/common/types/include",
    "${nn_src}/runtime",
    "${nn_src}/runtime/include",
    "${nn_src}/tools/test_generator/test_harness/include",
    "${sysroot}/usr/include/aosp",
    "//aosp/hardware/interfaces/neuralnetworks/1.0/utils/include/",
    "//aosp/hardware/interfaces/neuralnetworks/1.1/utils/include/",
    "//aosp/hardware/interfaces/neuralnetworks/1.2/utils/include/",
    "//aosp/hardware/interfaces/neuralnetworks/1.3/utils/include/",
    "//aosp/hardware/interfaces/neuralnetworks/utils/common/include/"
  ]
  pkg_deps = [
    "libnnapi-support",
  ]
}

static_library("vts_common") {
  configs += [
    ":vts_defaults",
    "//aosp/frameworks/ml:target_defaults",
    "//common-mk:test",
  ]
  cflags_cc = [
    # ExecutionBurstController has signed vs unsigned compares
    "-Wno-sign-compare",
    # Utils.cpp has unused parameters
    "-Wno-unused-parameter",
  ]
  deps = [
    "${nn_src}:nn-common",
    "${nn_src}:runtime",
    "${nn_src}:utils_common",
    "${nn_src}:utils_1_0",
    "${nn_src}:utils_1_1",
    "${nn_src}:utils_1_2",
    "${nn_src}:utils_1_3",
    "${nn_src}:vendor-nn-hal",
  ]
  include_dirs = [
    "${nn_src}/chromeos/config",
  ]
  sources = [
    "${vts_src}/1.0/vts/functional/Callbacks.cpp",
    "${vts_src}/1.0/vts/functional/Utils.cpp",
    "${nn_src}/chromeos/logger.cpp",
    "${nn_src}/chromeos/compilation_caching.cpp",
    "${nn_src}/chromeos/daemon_store.cpp",
    "${nn_src}/chromeos/testrunner.cpp",
    "${nn_src}/chromeos/testrunner_messageloop.cpp",
    "${nn_src}/chromeos/driver_loader/driver_loader.cpp",
    "${nn_src}/chromeos/driver_loader/driver_loader_utils.cpp",
    "${nn_src}/common/ExecutionBurstController.cpp",
    "${nn_src}/common/ExecutionBurstServer.cpp",
    "${nn_src}/common/LegacyHalUtils.cpp",
    "${nn_src}/common/LegacyUtils.cpp",
    "${nn_src}/common/MemoryUtils.cpp",
    "${nn_src}/common/types/src/SharedMemoryAndroid.cpp",
    "${nn_src}/common/types/src/TypeUtils.cpp",
    "${nn_src}/common/types/src/Types.cpp",
    "${nn_src}/common/types/src/Validation.cpp",
    "${nn_src}/tools/test_generator/test_harness/TestHarness.cpp",
  ]
}

executable("cros_nnapi_vts_1_0") {
  configs += [
    ":vts_defaults",
    "//aosp/frameworks/ml:target_defaults",
  ]
  deps = [
    ":vts_common",
    "${nn_src}/chromeos/tests/runtime/generated:generated_spec_v1_0",
